# Ignore bundler config.
.bundle/

# Ignore all logfiles and tempfiles.
log/
tmp/
!/log/.keep
!/tmp/.keep

.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key
/config/credentials/production.key
/config/credentials/scaling.key

# ignore bundler vendor/cache
vendor/

# credentials and environment settings
.aptible.env
.env
.env.dev.local
.env.local
.env.rediscloud_url
.env.test.local
postgres.env

# Ignore the default SQLite database.
db/test.sqlite3
db/*.sqlite3-journal

.DS_Store

# dev tools artifacts
.yardoc/
brakeman.log
coverage/
doc/
erd.pdf
jmeter.log
robots.txt
sitemap.xml

*.dump
*.rdb
node_modules/
public/assets/
public/system/

#Ignore IDE settings
.idea/
.vscode/

# store of failed examples
spec/examples.txt

# Cypress (if applicable)
uitests/cypress/videos
uitests/cypress/screenshots

# scaling input and output files
scaling/input
scaling/automated-tests

/config/credentials/pentest.key

/config/credentials/rnd-testing.key